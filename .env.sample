# External Services
SENDGRID_API_KEY=your-sendgrid-key
APPLICATION_INSIGHTS_KEY=your-insights-key

# Feature Flags
ENABLE_PDF_GENERATION=true
ENABLE_EMAIL_NOTIFICATIONS=true
